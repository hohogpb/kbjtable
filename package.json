{
  "name": "kbjtable",
  "author": "bh",
  "version": "0.0.2",
  "description": "a simple editable table in react",
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/kbjtable.esm.js",
  "typings": "dist/index.d.ts",
  "keywords": [    
    "kbjtable",
    "可编辑表格",
    "table",
    "editable",    
    "react"
  ],
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test --passWithNoTests",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "peerDependencies": {
    "react": ">=16",
    "@juggle/resize-observer": ">=3.4.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "size-limit": [
    {
      "path": "dist/kbjtable.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/kbjtable.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@juggle/resize-observer": "^3.4.0",
    "@size-limit/preset-small-lib": "^11.1.1",
    "@types/react": "^18.2.67",
    "@types/react-dom": "^18.2.22",
    "husky": "^9.0.11",
    "less": "^4.2.0",
    "postcss": "^8.4.36",
    "postcss-modules": "^6.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rollup-plugin-postcss": "^4.0.2",
    "sass": "^1.72.0",
    "size-limit": "^11.1.1",
    "tsdx": "^0.14.1",
    "tslib": "^2.6.2",
    "typescript": "^5.4.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hohogpb/kbjtable.git"
  },
  "bugs": {
    "url": "https://github.com/hohogpb/kbjtable/issues"
  },
  "homepage": "https://github.com/hohogpb/kbjtable#readme"
}
